<h1><%= @list.name %></h1>
<h3><%= @list.priority%></h3>
<br/>
<% @list.tasks.each do |task| %>
  <div>
    <h3><%= link_to task.name, [@list, task] %></h3>
  </div>
<% end %>
<br/>
<br/>

<%= link_to 'Add Task', new_list_task_path(@list), class: "ui blue button" %>

<%= link_to 'Delete List', board_list_path(@board, @list), method: :delete, class: "ui red button" %>

